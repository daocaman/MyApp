<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.lang.String?>
<BorderPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <fx:include source="Header.fxml"/>
    </top>
    <center>
        <TabPane prefHeight="200.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Moving File">
                    <content>
                        <fx:include source="MovingFile.fxml"/>
                    </content>
                </Tab>
                <Tab text="Book Manager">
                    <content>
                        <BorderPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab text="Number file">
                    <content>
                        <fx:include source="NumberFile.fxml"/>
                    </content>
                </Tab>
                <Tab text="Remove other file">
                    <content>
                        <fx:include source="RemoveOther.fxml"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
    <bottom>
        <fx:include source="Footer.fxml"/>
    </bottom>
</BorderPane>
